    def substract_p1(self):
        self.score1 = max(0,self.score1-1)
        self.score1_value.config(state=NORMAL)
        self.score1_value.delete(0, END)
        self.score1_value.insert(0, self.score1)
        self.score1_value.config(state=DISABLED)
        
    def reset_p1(self):
