def exists_in_file(file, string):
    file = open(file,'r')
    for line in file:
        line = line.strip()
        if string == line:
            file.close()
            return True
    else:
        file.close()
        return False

def replace_file(file,string):
    file = open(file,'w')
    file.write(string)
    file.close()

def append_to_file(file,string):
    file = open(file,'a')
    file.write(string+"\n")
    file.close()

def list_database(file):
    entries = []
    file = open(file,'r')
    for line in file:
        line = line.strip()
        entries.append(line)
    return entries

